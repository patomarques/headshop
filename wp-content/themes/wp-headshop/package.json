{
  "name": "storefront-child",
  "version": "1.0.0",
  "private": true,
  "description": "Build scripts for Storefront child theme SCSS",
  "scripts": {
    "build": "sass --no-source-map --style=expanded assets/sass/main.scss assets/css/main.css",
    "watch": "sass --watch assets/sass/main.scss:assets/css/main.css"
  },
  "devDependencies": {
    "sass": "^1.80.0"
  }
}
